    "d3-format": "^1.3.2",
    "d3-time-format": "^2.1.3",
    "lodash": "^4.17.11",
    "moment": "2.24.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-c3js": "^0.1.20",
